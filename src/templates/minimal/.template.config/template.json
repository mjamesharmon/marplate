{
  "$schema": "http://json.schemastore.org/template",
  "author": "M.James Harmon",
  "classifications": [ "Presentation", "Template" ],
  "identity": "MarpTemplate.Minimal",
  "name": "Minimal Marp Template",
  "shortName": "marp.minimal",
  "tags": {
    "language": "markdown",
    "type": "item"
  },
  "sourceName": "MarpTemplate",
  "preferNameDirectory":true,
  "symbols":{
    "primaryColor": {
      "type": "parameter",
      "defaultValue": "#1B9CDF;",
      "replaces":"PColor"
    },
    "secondaryColor": {
      "type": "parameter",
      "defaultValue": "#CCCCCC;",
      "replaces":"SColor"
    },
    "tertiaryColor": {
      "type": "parameter",
      "defaultValue": "#FFBD3E",
      "replaces":"TColor"
    }

}
